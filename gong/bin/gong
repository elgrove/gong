#!/usr/bin/env python3

# if arg is init, create site skeleton
# if arg is build, check skeleton exists, build, if not then error
################################################################################### if arg is serve, start server with site directory

import typer

# from gong import init, build
import os

app = typer.Typer()


@app.command()
def init():
    typer.echo("init")


@app.command()
def build():
    typer.echo("build")


if __name__ == "__main__":
    app()
